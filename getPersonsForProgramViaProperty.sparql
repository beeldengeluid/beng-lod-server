#+ summary: Query to get all persons associated with a program by a specific property
#+ endpoint: http://zorin.beeldengeluid.nl:3020/sparql/
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX nisv: <http://data.rdlabs.beeldengeluid.nl/schema/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

SELECT DISTINCT ?person ?personName WHERE
{
?_o1_iri ?_personProperty_iri ?personInRole . ?personInRole nisv:hasPerson ?person . ?person skos:prefLabel ?personName
}
ORDER BY ?personName

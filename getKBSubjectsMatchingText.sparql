#+ summary: Query to get 10 subjects from the KB that match some text (with a regex)
#+ endpoint: http://data.bibliotheken.nl/sparql

SELECT DISTINCT ?subjectId ?subjectName where 
{?Concept <http://schema.org/about> ?subjectId . ?subjectId skos:prefLabel ?subjectName filter regex(str(?subjectName), ?_text, "i")} LIMIT 10 
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sdo: <https://schema.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .


### IN PROGRESS: schema not yet complete!

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .

#################################################################
#    Object Properties
#################################################################

###  https://schema.org/about
sdo:about rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:Thing .

###  https://schema.org/actor
sdo:actor rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:Person .

###  https://schema.org/additionalType
sdo:additionalType rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:URL .

###  https://schema.org/alternativeHeadline
sdo:alternativeHeadline rdf:type rdf:Property ;
                rdfs:domain sdo:CreativeWork ;
                rdfs:range xsd:string .

###  https://schema.org/associatedMedia
sdo:associatedMedia rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:MediaObject.

###  https://schema.org/byArtist - used for performers
sdo:byArtist rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:Person .

###  https://schema.org/contentUrl
sdo:contentUrl rdf:type rdf:Property ;
                rdfs:domain sdo:MediaObject ;
                rdfs:range sdo:URL .

###  https://schema.org/contributor - used for guests
sdo:contributor rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:Person .

###  https://schema.org/creator
sdo:creator rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:Person .

###  https://schema.org/genre
sdo:genre rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:Thing .

###  https://schema.org/hasPart
sdo:hasPart rdf:type rdf:Property ;
                rdfs:domain sdo:CreativeWork ;
                rdfs:range sdo:Clip .

###  https://schema.org/identifier
sdo:identifier rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range xsd:string .

###  https://schema.org/inLanguage
sdo:inLanguage rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range xsd:string .

###  https://schema.org/location
sdo:location rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:Place .

###  https://schema.org/locationCreated  # used for recording location
sdo:locationCreated rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:Place .

###  https://schema.org/mainEntityOfPage
sdo:mainEntityOfPage rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:URL .

###  https://schema.org/mentions - used for persons and corporations
sdo:mentions rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:Thing .

###  https://schema.org/partOfSeason
sdo:partOfSeason rdf:type rdf:Property ;
                rdfs:domain sdo:CreativeWork ;
                rdfs:range sdo:CreativeWorkSeason .

###  https://schema.org/partOfSeries
sdo:partOfSeries rdf:type rdf:Property ;
                rdfs:domain sdo:CreativeWork ;
                rdfs:domain sdo:CreativeWorkSeason ;
                rdfs:range sdo:CreativeWorkSeries .

###  https://schema.org/productionCompany
sdo:productionCompany rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:Organization .

###  https://schema.org/publisher
sdo:publisher rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:Thing .  # can be Person or Organization for us

###  https://schema.org/provider  # used for broadcaster
sdo:provider rdf:type rdf:Property ;
                rdfs:domain sdo:CreativeWork ;
                rdfs:range sdo:Organization .

###  https://schema.org/recordedAt  # used for event
sdo:recordedAt rdf:type rdf:Property ;
                rdfs:domain sdo:CreativeWork ;
                rdfs:range sdo:Event .

###  https://schema.org/roleName
sdo:roleName rdf:type rdf:Property ;
                rdfs:domain sdo:Role ;
                rdfs:range xsd:string .

###  https://schema.org/url
sdo:url rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range sdo:URL .

#################################################################
#    Data properties
#################################################################

###  https://schema.org/conditionsOfAccess
sdo:conditionsOfAccess rdf:type rdf:Property ;
                rdfs:domain sdo:CreativeWork ;
                rdfs:range xsd:string .

######  https://schema.org/description  - NB: currently unmapped as descriptions are under copyright
###sdo:description rdf:type rdf:Property ;
###                rdfs:domain sdo:Thing ;
###                rdfs:range xsd:string .

###  https://schema.org/datePublished
sdo:datePublished rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range xsd:string .

###  https://schema.org/duration
sdo:duration rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range xsd:string .

###  https://schema.org/encodingFormat
sdo:encodingFormat rdf:type rdf:Property ;
                rdfs:domain sdo:MediaObject;
                rdfs:range xsd:string .

###  https://schema.org/keywords
sdo:keywords rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range xsd:string .

###  https://schema.org/license
sdo:license rdf:type rdf:Property ;
                rdfs:domain sdo:CreativeWork ;
                rdfs:range xsd:string .

###  https://schema.org/material
sdo:material rdf:type rdf:Property ;
                rdfs:domain sdo:CreativeWork ;
                rdfs:range xsd:string .

###  https://schema.org/name
sdo:name rdf:type rdf:Property ;
                rdfs:domain sdo:Thing ;
                rdfs:range xsd:string .

#################################################################
#    Classes
#################################################################

###  https://schema.org/AudioObject
sdo:AudioObject rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:MediaObject .

###  https://schema.org/Clip
sdo:Clip rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:CreativeWork .

###  https://schema.org/CreativeWork
sdo:CreativeWork rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:Thing .

###  https://schema.org/CreativeWorkSeason
sdo:CreativeWorkSeason rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:CreativeWork .

###  https://schema.org/CreativeWorkSeries
sdo:CreativeWorkSeries rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:CreativeWork .

###  https://schema.org/Event
sdo:Event rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:Thing ;
             sdo:additionalType skos:Concept .

###  https://schema.org/ImageObject
sdo:ImageObject rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:MediaObject .

###  https://schema.org/MediaObject
sdo:MediaObject rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:Thing .

###  https://schema.org/Organization
sdo:Organization rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:Thing ;
             sdo:additionalType skos:Concept .

###  https://schema.org/PerformanceRole
sdo:PerformanceRole rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:Role .

###  https://schema.org/Person
sdo:Person rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:Thing ;
             sdo:additionalType skos:Concept .

###  https://schema.org/Place
sdo:Place rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:Thing ;
             sdo:additionalType skos:Concept .

###  https://schema.org/Role
sdo:Role rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:Thing .

###  https://schema.org/Thing
sdo:Thing rdf:type rdfs:Class ;
    sdo:additionalType skos:Concept.

###  https://schema.org/URL
sdo:URL rdf:type rdfs:Class ;
    rdfs:subClassOf sdo:Text .

###  https://schema.org/VideoObject
sdo:VideoObject rdf:type rdfs:Class ;
             rdfs:subClassOf sdo:MediaObject .

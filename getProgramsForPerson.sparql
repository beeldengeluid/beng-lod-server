#+ summary: Query to get the title and ID of all programs associated with a person (at present only via creator relations)
#+ endpoint: http://zorin.beeldengeluid.nl:3020/sparql/

PREFIX schema: <http://data.rdlabs.beeldengeluid.nl/schema/>

SELECT DISTINCT ?programId ?title
WHERE {
?programId a schema:Program . OPTIONAL {?programId schema:hasMainTitle ?titleObject . ?titleObject schema:titleValue ?title .} ?programId schema:creator ?personId . ?personId schema:name ?_personName
}
